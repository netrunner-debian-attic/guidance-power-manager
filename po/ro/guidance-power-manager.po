msgid "The AC adapter has been plugged in, switching to AC mode."
msgstr "Adaptorul de alimentare a fost atașat, comut la regimul AC."

#: guidance-power-manager.py:1006
msgid "You are about to run out of battery power, doing nothing now."
msgstr ""
"Sînteți pe cale de a rămîne fără energie în acumulator, nu fac nimic acum."

#: guidance-power-manager.py:1008
msgid "You are about to run out of battery power, blanking screen now."
msgstr ""
"Sînteți pe cale de a rămîne fără energie în acumulator, golesc ecranul acum."

#: guidance-power-manager.py:1010
msgid "You are about to run out of battery power, suspending now."
msgstr "Sînteți pe cale de a rămîne fără energie în acumulator, suspend acum."

#: guidance-power-manager.py:1012
msgid "You are about to run out of battery power, hibernating now."
msgstr "Sînteți pe cale de a rămîne fără energie în acumulator, hibernez acum."

#: guidance-power-manager.py:1014
msgid "You are about to run out of battery power, shutting down now."
msgstr "Sînteți pe cale de a rămîne fără energie în acumulator, opresc acum."

#: guidance-power-manager.py:1023
msgid "You are low on battery power."
msgstr "Aveți puțină energie în acumulator."

#: guidance-power-manager.py:1051
msgid "System idle for at least %1 minute, doing nothing now."
msgid_plural "System idle for at least %1 minutes, doing nothing now."
msgstr[0] "Sistem inactiv pentru cel puțin %1 minut, nu fac nimic acum."
msgstr[1] "Sistem inactiv pentru cel puțin %1 minute, nu fac nimic acum."
msgstr[2] "Sistem inactiv pentru cel puțin %1 de minute, nu fac nimic acum."

#: guidance-power-manager.py:1053
msgid "System idle for at least %1 minute, blanking screen now."
msgid_plural "System idle for at least %1 minutes, blanking screen now."
msgstr[0] "Sistem inactiv pentru cel puțin %1 minut, golesc ecranul acum."
msgstr[1] "Sistem inactiv pentru cel puțin %1 minute, golesc ecranul acum."
msgstr[2] "Sistem inactiv pentru cel puțin %1 de minute, golesc ecranul acum."

#: guidance-power-manager.py:1055
msgid "System idle for at least %1 minute, suspending now."
msgid_plural "System idle for at least %1 minutes, suspending now."
msgstr[0] "Sistem inactiv pentru cel puțin %1 minut, suspend acum."
msgstr[1] "Sistem inactiv pentru cel puțin %1 minute, suspend acum."
msgstr[2] "Sistem inactiv pentru cel puțin %1 de minute, suspend acum."

#: guidance-power-manager.py:1057
msgid "System idle for at least %1 minute, hibernating now."
msgid_plural "System idle for at least %1 minutes, hibernating now."
msgstr[0] "Sistem inactiv pentru cel puțin %1 minut, hibernez acum."
msgstr[1] "Sistem inactiv pentru cel puțin %1 minute, hibernez acum."
msgstr[2] "Sistem inactiv pentru cel puțin %1 de minute, hibernez acum."

#: guidance-power-manager.py:1059
msgid "System idle for at least %1 minute, shutting down now."
msgid_plural "System idle for at least %1 minutes, shutting down now."
msgstr[0] "Sistem inactiv pentru cel puțin %1 minut, închid acum."
msgstr[1] "Sistem inactiv pentru cel puțin %1 minute, închid acum."
msgstr[2] "Sistem inactiv pentru cel puțin %1 de minute, închid acum."

