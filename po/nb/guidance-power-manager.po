msgstr "Batteriet er snart tomt for energi, går i dvalemodus nå."

#: guidance-power-manager.py:1014
msgid "You are about to run out of battery power, shutting down now."
msgstr "Batteriet er snart tomt for energi, slår av maskinen nå."

#: guidance-power-manager.py:1023
msgid "You are low on battery power."
msgstr "Det er lite igjen i batteriet."

#: guidance-power-manager.py:1051
msgid "System idle for at least %1 minute, doing nothing now."
msgid_plural "System idle for at least %1 minutes, doing nothing now."
msgstr[0] "Systemet i tomgang i minst %1 minutt, ingenting gjøres nå."
msgstr[1] "Systemet i tomgang i minst %1 minutter, ingenting gjøres nå."

#: guidance-power-manager.py:1053
msgid "System idle for at least %1 minute, blanking screen now."
msgid_plural "System idle for at least %1 minutes, blanking screen now."
msgstr[0] "Systemet i tomgang i minst %1 minutt, blanker ut skjermen nå."
msgstr[1] "Systemet i tomgang i minst %1 minutter, blanker ut skjermen nå."

#: guidance-power-manager.py:1055
msgid "System idle for at least %1 minute, suspending now."
msgid_plural "System idle for at least %1 minutes, suspending now."
msgstr[0] "Systemet i tomgang i minst %1 minutt, går i hvilemodus nå."
msgstr[1] "Systemet i tomgang i minst %1 minutter, går i hvilemodus nå."

#: guidance-power-manager.py:1057
msgid "System idle for at least %1 minute, hibernating now."
msgid_plural "System idle for at least %1 minutes, hibernating now."
msgstr[0] "Systemet i tomgang i minst %1 minutt, går i dvalemodus nå."
msgstr[1] "Systemet i tomgang i minst %1 minutter, går i dvalemodus nå."

#: guidance-power-manager.py:1059
msgid "System idle for at least %1 minute, shutting down now."
msgid_plural "System idle for at least %1 minutes, shutting down now."
msgstr[0] "Systemet i tomgang i minst %1 minutt, slår av nå."
msgstr[1] "Systemet i tomgang i minst %1 minutter, slår av nå."

